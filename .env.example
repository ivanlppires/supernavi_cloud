# Server
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Database
DATABASE_URL=postgresql://supernavi:supernavi_dev@localhost:5432/supernavi_cloud

# Wasabi S3 (or compatible)
S3_ENDPOINT=https://s3.eu-central-1.wasabisys.com
S3_REGION=eu-central-1
S3_BUCKET=supernavi-eu
S3_ACCESS_KEY=your-access-key
S3_SECRET_KEY=your-secret-key
S3_FORCE_PATH_STYLE=true

# Signed URL settings
SIGNED_URL_TTL_SECONDS=120

# Edge Tunnel Authentication
# Token that edge agents must provide to connect (generate with: openssl rand -base64 32)
EDGE_TUNNEL_TOKEN=your-secure-token-here

# Edge Tunnel Timeouts (milliseconds)
EDGE_TUNNEL_HEALTH_TIMEOUT_MS=2000
EDGE_TUNNEL_TILE_TIMEOUT_MS=8000

# JWT Authentication (generate with: openssl rand -base64 32)
JWT_SECRET=dev-jwt-secret-change-in-production
JWT_EXPIRES_IN=7d

# Google OAuth
GOOGLE_CLIENT_ID=

# UI-Bridge (Chrome Extension) Authentication (generate with: openssl rand -base64 32)
UI_BRIDGE_API_KEY=

# Magic Link JWT (generate with: openssl rand -base64 32)
MAGIC_LINK_SECRET=
MAGIC_LINK_TTL_SECONDS=300

# Thumb signing (HMAC) - defaults to MAGIC_LINK_SECRET if not set
THUMB_SIGN_SECRET=
THUMB_SIGN_TTL_SECONDS=600

# Frontend URL for magic links
FRONTEND_URL=http://localhost:3002

# CORS allowed origins (comma-separated, leave empty to allow all in dev)
# Production example: https://viewer.supernavi.app,https://pathoweb.com.br
ALLOWED_ORIGINS=
